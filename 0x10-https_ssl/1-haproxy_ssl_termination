frontend thore.tech
        bind *:80
        bind *:443 ssl crt /etc/haproxy/certs/www.thore.tech.pem
        redirect scheme https code 301 if !{ ssl_fc }
        default_backend web_servers

backend web_servers
        mode http
        balance roundrobin
        server 339486-web-01 3.84.239.148:80 check
        server 339486-web-02 100.26.243.121:80 check
